<apex:page controller="EditShipToAddressController">
<!-- Begin Default Content REMOVE THIS -->
<h1>Edit a shipping address 
</h1>
<!-- End Default Content REMOVE THIS -->
<apex:form id="editShipTo">
        <apex:pageBlock title="Edit Shipping Address">
            <apex:pageBlockSection title="param testing">
                <apex:inputText value="{!tempId}">id: </apex:inputText>
            </apex:pageBlockSection>
            <apex:pageBlockSection title="Shipping Information" columns="2">
                <apex:inputText value="{!updatedShipTo.firstName}" name="firstName">First Name: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.lastName}" name="lastName">Last Name: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.address}">Address: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.city}">City: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.state}">State: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.zip}">Zip: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.country}">Country: </apex:inputText>
                <apex:inputText value="{!updatedShipTo.phoneNumber}">Phone Number: </apex:inputText>
            </apex:pageBlockSection>
        </apex:pageBlock>
       
        <apex:commandButton value="Submit" action="{!editShipTo}" >
        </apex:commandButton>
            <apex:commandButton value="Cancel" action="{!redirectToViewShipToAddress}"/>
    </apex:form>
</apex:page>
